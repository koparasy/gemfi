////////////////////////////////////////////////////////////////////
//
// The actual decoder specification
//

decode OPCODE default Unknown::unknown() {

    format LoadAddress {
      0x08: lda({{ uint64_t tmp  = Rb + disp; 
		    if(true){
			fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		    }
		    Ra = tmp;
	
      }});
      0x09: ldah({{ uint64_t tmp = Rb + (disp << 16); 
		    if(true){
		      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		    }
		    Ra=tmp;
		    }});
    }

    format LoadOrNop {
        0x0a: ldbu({{ Ra_uq = Mem_ub; }});
        0x0c: ldwu({{ Ra_uq = Mem_uw; }});
	0x0b: ldq_u({{ Ra = Mem_uq; }}, ea_code = {{ EA = (Rb + disp) & ~7; if(true) fi_system->iew_fault(xc, xc->tcBase(),&EA); }});
        0x23: ldt({{ Fa = Mem_df; }});
        0x2a: ldl_l({{ Ra_sl = Mem_sl; }}, mem_flags = LLSC);
        0x2b: ldq_l({{ Ra_uq = Mem_uq; }}, mem_flags = LLSC);
    }

    format LoadOrPrefetch {
        0x28: ldl({{ Ra_sl = Mem_sl; }});
        0x29: ldq({{ Ra_uq = Mem_uq; }}, pf_flags = EVICT_NEXT);
        // IsFloating flag on lds gets the prefetch to disassemble
        // using f31 instead of r31... funcitonally it's unnecessary
        0x22: lds({{ Fa_uq = s_to_t(Mem_ul); }},
                  pf_flags = PF_EXCLUSIVE, inst_flags = IsFloating);
    }

    format Store {
        0x0e: stb({{ Mem_ub = Ra<7:0>; }});
        0x0d: stw({{ Mem_uw = Ra<15:0>; }});
        0x2c: stl({{ Mem_ul = Ra<31:0>; }});
        0x2d: stq({{ Mem_uq = Ra_uq; }});
	0x0f: stq_u({{ Mem_uq = Ra_uq; }}, {{ EA = (Rb + disp) & ~7; if(true) fi_system->iew_fault(xc, xc->tcBase(),&EA); }});
        0x26: sts({{ Mem_ul = t_to_s(Fa_uq); }});
        0x27: stt({{ Mem_df = Fa; }});
    }

    format StoreCond {
        0x2e: stl_c({{ Mem_ul = Ra<31:0>; }},
                    {{
                        uint64_t tmp = write_result;
                        // see stq_c
                        Ra = (tmp == 0 || tmp == 1) ? tmp : Ra;
                        if (tmp == 1) {
                            xc->setStCondFailures(0);
                        }
                    }}, mem_flags = LLSC, inst_flags = IsStoreConditional);
        0x2f: stq_c({{ Mem_uq = Ra; }},
                    {{
                        uint64_t tmp = write_result;
                        // If the write operation returns 0 or 1, then
                        // this was a conventional store conditional,
                        // and the value indicates the success/failure
                        // of the operation.  If another value is
                        // returned, then this was a Turbolaser
                        // mailbox access, and we don't update the
                        // result register at all.
                        Ra = (tmp == 0 || tmp == 1) ? tmp : Ra;
                        if (tmp == 1) {
                            // clear failure counter... this is
                            // non-architectural and for debugging
                            // only.
                            xc->setStCondFailures(0);
                        }
                    }}, mem_flags = LLSC, inst_flags = IsStoreConditional);
    }

    format IntegerOperate {

        0x10: decode INTFUNC {  // integer arithmetic operations

        0x00: addl({{ int32_t tmp = Ra_sl + Rb_or_imm_sl; 
	      if(true)
		fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	      Rc_sl = tmp;
	}});
            0x40: addlv({{
                int32_t tmp  = Ra_sl + Rb_or_imm_sl;
                // signed overflow occurs when operands have same sign
                // and sign of result does not match.
                if (Ra_sl<31:> == Rb_or_imm_sl<31:> && tmp<31:> != Ra_sl<31:>)
                    fault = new IntegerOverflowFault;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
                Rc_sl = tmp;
            }});
            0x02: s4addl({{ int32_t tmp = (Ra_sl << 2) + Rb_or_imm_sl;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc_sl=tmp;
	    }});
	    0x12: s8addl({{ int32_t tmp = (Ra_sl << 3) + Rb_or_imm_sl;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc_sl = tmp;
	    }});

	    0x20: addq({{ uint64_t tmp = Ra + Rb_or_imm;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
            0x60: addqv({{
                uint64_t tmp = Ra + Rb_or_imm;
                // signed overflow occurs when operands have same sign
                // and sign of result does not match.
                if (Ra<63:> == Rb_or_imm<63:> && tmp<63:> != Ra<63:>)
                    fault = new IntegerOverflowFault;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
                Rc = tmp;
            }});
	    0x22: s4addq({{ uint64_t tmp = (Ra << 2) + Rb_or_imm;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
            0x32: s8addq({{ uint64_t tmp = (Ra << 3) + Rb_or_imm; 
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});

	    0x09: subl({{ int32_t tmp = Ra_sl - Rb_or_imm_sl;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc_sl = tmp;
	    }});
            0x49: sublv({{
                int32_t tmp  = Ra_sl - Rb_or_imm_sl;
                // signed overflow detection is same as for add,
                // except we need to look at the *complemented*
                // sign bit of the subtrahend (Rb), i.e., if the initial
                // signs are the *same* then no overflow can occur
                if (Ra_sl<31:> != Rb_or_imm_sl<31:> && tmp<31:> != Ra_sl<31:>)
                    fault = new IntegerOverflowFault;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
                Rc_sl = tmp;
            }});
	    0x0b: s4subl({{ int32_t tmp = (Ra_sl << 2) - Rb_or_imm_sl;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc_sl = tmp;
	    }});
	    0x1b: s8subl({{ int32_t tmp = (Ra_sl << 3) - Rb_or_imm_sl;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc_sl = tmp;
	    }});

	    0x29: subq({{ uint64_t tmp = Ra - Rb_or_imm;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
            0x69: subqv({{
                uint64_t tmp  = Ra - Rb_or_imm;
                // signed overflow detection is same as for add,
                // except we need to look at the *complemented*
                // sign bit of the subtrahend (Rb), i.e., if the initial
                // signs are the *same* then no overflow can occur
                if (Ra<63:> != Rb_or_imm<63:> && tmp<63:> != Ra<63:>)
                    fault = new IntegerOverflowFault;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
	    0x2b: s4subq({{ uint64_t tmp  = (Ra << 2) - Rb_or_imm;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	     Rc = tmp;
	    }});
	    0x3b: s8subq({{ uint64_t tmp  = (Ra << 3) - Rb_or_imm;
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	     Rc = tmp;
	    }});

	    0x2d: cmpeq({{ uint64_t tmp = (Ra == Rb_or_imm);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x6d: cmple({{ uint64_t tmp = (Ra_sq <= Rb_or_imm_sq);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x4d: cmplt({{ uint64_t tmp = (Ra_sq <  Rb_or_imm_sq);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x3d: cmpule({{ uint64_t tmp = (Ra_uq <= Rb_or_imm_uq);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x1d: cmpult({{ uint64_t tmp = (Ra_uq <  Rb_or_imm_uq);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});

            0x0f: cmpbge({{
                int hi = 7;
                int lo = 0;
                uint64_t tmp = 0;
                for (int i = 0; i < 8; ++i) {
                    tmp |= (Ra_uq<hi:lo> >= Rb_or_imm_uq<hi:lo>) << i;
                    hi += 8;
                    lo += 8;
                }
                if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
                Rc = tmp;
            }});
        }

        0x11: decode INTFUNC {  // integer logical operations

        0x00: and({{ uint64_t tmp  = Ra & Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;
	}});
	0x08: bic({{ uint64_t tmp  = Ra & ~Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;
	}});
	0x20: bis({{ uint64_t tmp  = Ra | Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;
	}});
	0x28: ornot({{ uint64_t tmp  = Ra | ~Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;  
	}});
	0x40: xor({{ uint64_t tmp  = Ra ^ Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;
	}});
	0x48: eqv({{ uint64_t tmp  = Ra ^ ~Rb_or_imm;
	if(true)
	  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	Rc = tmp;
	}});

            // conditional moves
	0x14: cmovlbs({{ uint64_t tmp = ((Ra & 1) == 1) ? Rb_or_imm : Rc;
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x16: cmovlbc({{ uint64_t tmp = ((Ra & 1) == 0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x24: cmoveq({{ uint64_t tmp = (Ra == 0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x26: cmovne({{ uint64_t tmp = (Ra != 0) ? Rb_or_imm : Rc;
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x44: cmovlt({{ uint64_t tmp = (Ra_sq <  0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x46: cmovge({{ uint64_t tmp = (Ra_sq >= 0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x64: cmovle({{ uint64_t tmp = (Ra_sq <= 0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	0x66: cmovgt({{ uint64_t tmp = (Ra_sq >  0) ? Rb_or_imm : Rc; 
// 	    if(true)
// 	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});

            // For AMASK, RA must be R31.
            0x61: decode RA {
	      31: amask({{ uint64_t tmp = Rb_or_imm & ~ULL(0x17);
	      if(true)
		fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	      Rc = tmp;
	      }});
            }

            // For IMPLVER, RA must be R31 and the B operand
            // must be the immediate value 1.
            0x6c: decode RA {
                31: decode IMM {
                    1: decode INTIMM {
                        // return EV5 for FullSystem and EV6 otherwise
                        1: implver({{ uint64_t tmp = FullSystem ? 1 : 2; 
                        if(true)
			  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
			Rc = tmp;
			}});
                    }
                }
            }

            // The mysterious 11.25...
            0x25: WarnUnimpl::eleven25();
        }

        0x12: decode INTFUNC {
	  0x39: sll({{ uint64_t tmp = Ra << Rb_or_imm<5:0>;
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});
	  0x34: srl({{ uint64_t tmp = Ra_uq >> Rb_or_imm<5:0>;
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});
	  0x3c: sra({{ uint64_t tmp = Ra_sq >> Rb_or_imm<5:0>;
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});

	  0x02: mskbl({{ uint64_t tmp = Ra & ~(mask( 8) << (Rb_or_imm<2:0> * 8));
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});
	  0x12: mskwl({{ uint64_t tmp = Ra & ~(mask(16) << (Rb_or_imm<2:0> * 8));
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});
	  0x22: mskll({{ uint64_t tmp = Ra & ~(mask(32) << (Rb_or_imm<2:0> * 8));
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});
	  0x32: mskql({{ uint64_t tmp = Ra & ~(mask(64) << (Rb_or_imm<2:0> * 8));
	  if(true)
	    fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	  Rc = tmp;
	  }});

            0x52: mskwh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp =  bv ? (Ra & ~(mask(16) >> (64 - 8 * bv))) : Ra;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
            0x62: msklh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp =  bv ? (Ra & ~(mask(32) >> (64 - 8 * bv))) : Ra;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
            0x72: mskqh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp =  bv ? (Ra & ~(mask(64) >> (64 - 8 * bv))) : Ra;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
	    0x06: extbl({{ uint64_t tmp = (Ra_uq >> (Rb_or_imm<2:0> * 8))< 7:0>; 
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x16: extwl({{ uint64_t tmp = (Ra_uq >> (Rb_or_imm<2:0> * 8))<15:0>; 
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x26: extll({{ uint64_t tmp = (Ra_uq >> (Rb_or_imm<2:0> * 8))<31:0>; 
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x36: extql({{ uint64_t tmp = (Ra_uq >> (Rb_or_imm<2:0> * 8)); 
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});

            0x5a: extwh({{
	      uint64_t tmp = (Ra << (64 - (Rb_or_imm<2:0> * 8))<5:0>)<15:0>; 
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
	    }});
            0x6a: extlh({{
	      uint64_t tmp = (Ra << (64 - (Rb_or_imm<2:0> * 8))<5:0>)<31:0>; 
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
	    }});
            0x7a: extqh({{
	      uint64_t tmp = (Ra << (64 - (Rb_or_imm<2:0> * 8))<5:0>); 
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
	    }});

	    0x0b: insbl({{ uint64_t tmp = Ra< 7:0> << (Rb_or_imm<2:0> * 8);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x1b: inswl({{ uint64_t tmp = Ra<15:0> << (Rb_or_imm<2:0> * 8); 
	      if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
	    }});
	    0x2b: insll({{ uint64_t tmp = Ra<31:0> << (Rb_or_imm<2:0> * 8);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});
	    0x3b: insql({{ uint64_t tmp = Ra       << (Rb_or_imm<2:0> * 8);
	    if(true)
	      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
	    Rc = tmp;
	    }});

            0x57: inswh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp = bv ? (Ra_uq<15:0> >> (64 - 8 * bv)) : 0;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
            0x67: inslh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp = bv ? (Ra_uq<31:0> >> (64 - 8 * bv)) : 0;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
            0x77: insqh({{
                int bv = Rb_or_imm<2:0>;
		uint64_t tmp = bv ? (Ra_uq       >> (64 - 8 * bv)) : 0;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});

            0x30: zap({{
                uint64_t zapmask = 0;
                for (int i = 0; i < 8; ++i) {
                    if (Rb_or_imm<i:>)
                        zapmask |= (mask(8) << (i * 8));
                }
                uint64_t tmp = Ra & ~zapmask;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
            0x31: zapnot({{
                uint64_t zapmask = 0;
                for (int i = 0; i < 8; ++i) {
                    if (!Rb_or_imm<i:>)
                        zapmask |= (mask(8) << (i * 8));
                }
                uint64_t tmp = Ra & ~zapmask;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		Rc = tmp;
            }});
        }

        0x13: decode INTFUNC {  // integer multiplies
        0x00: mull({{ int32_t tmp = Ra_sl * Rb_or_imm_sl;
		      if(true)
			fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		      Rc_sl = tmp;
	}}, IntMultOp);
	0x20: mulq({{ uint64_t tmp = Ra    * Rb_or_imm;  
		    if(true)
		      fi_system->iew_fault(xc, xc->tcBase(),&tmp);
		    Rc   =tmp;
	}}, IntMultOp);
            0x30: umulh({{
                uint64_t hi, lo;
                mul128(Ra, Rb_or_imm, hi, lo);
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&hi);
                Rc = hi;
            }}, IntMultOp);
            0x40: mullv({{
                // 32-bit multiply with trap on overflow
                int64_t Rax = Ra_sl;    // sign extended version of Ra_sl
                int64_t Rbx = Rb_or_imm_sl;
                int64_t tmp = Rax * Rbx;
                // To avoid overflow, all the upper 32 bits must match
                // the sign bit of the lower 32.  We code this as
                // checking the upper 33 bits for all 0s or all 1s.
                uint64_t sign_bits = tmp<63:31>;
                if (sign_bits != 0 && sign_bits != mask(33))
                    fault = new IntegerOverflowFault;
		int32_t tmp1 = tmp<31:0>;
		if(true)
		  fi_system->iew_fault(xc, xc->tcBase(),&tmp1);
                Rc_sl = tmp1;
            }}, IntMultOp);
            0x60: mulqv({{
                // 64-bit multiply with trap on overflow
                uint64_t hi, lo;
